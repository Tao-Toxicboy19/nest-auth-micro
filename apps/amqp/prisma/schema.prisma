generator client {
  provider    = "prisma-client-js"
  dotenv_path = "../amqp/.env"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

model Users {
  username   String   @unique
  password   String
  email      String   @unique
  login      Boolean  @default(false)
  created_at DateTime @default(now())
  updated_at DateTime
  id         String   @id
}
